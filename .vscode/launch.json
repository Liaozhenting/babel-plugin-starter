  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "babel",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "stopOnEntry": false,
      "sourceMaps": true,
      "args": [
       "dev", "--out-dir", "dist"  
      ],
      "program": "${workspaceFolder}/node_modules/babel-cli/bin/babel.js"
    }
  ]
}